package Day1;

public class Switch {
	public static void main(String[] args) {
		int a=2;
		
	switch(a) {
	case 1:
		System.out.println("a+a");
		break;
	case 2:
		System.out.println("a-a");
		break;
	case 3:
		System.out.println("a*a");
		break;
	case 4:
		System.out.println("DIVISION(a/a)");
		break;
	default:
		System.out.println("CALCULATE 2 NUMBERS");
	
		
		}
			
	}

}
